<p id="notice"><%= notice %></p>

<h1>Listing Service Configs</h1>

<table>
  <thead>
    <tr>
      <th>Ip address</th>
      <th>Config json</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_configs.each do |service_config| %>
      <tr>
        <td><%= service_config.ip_address %></td>
        <td><%= service_config.config_json %></td>
        <td><%= link_to 'Show', service_config %></td>
        <td><%= link_to 'Edit', edit_service_config_path(service_config) %></td>
        <td><%= link_to 'Destroy', service_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service config', new_service_config_path %>
