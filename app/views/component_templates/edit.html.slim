.row.justify-content-center
  .col-md-6
    h3 Edit Component Template
    - if flash.key?(:messages)
      .alert.alert-danger
        b Issue editing component template
        br
        - flash[:messages].each do |msg|
          = "- #{msg}"
          br
    #edit_component_template
      = render 'form', component_template: @component_template
